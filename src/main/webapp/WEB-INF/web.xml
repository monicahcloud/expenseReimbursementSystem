<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://JAVA.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
>
	<display-name>ExpenseReimbursementSystem</display-name>
	
	<welcome-file-list>
		<welcome-file>./html/landing.html</welcome-file>
	</welcome-file-list>
	
 
  <servlet>
  	<servlet-name>MasterViewServlet</servlet-name>
  	<servlet-class>com.example.servlet.MasterViewServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MasterViewServlet</servlet-name>
  	<url-pattern>/landing</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>MasterJSONServlet</servlet-name>
  	<servlet-class>com.example.servlet.MasterJSONServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MasterJSONServlet</servlet-name>
  	<!-- We can use the * to specify multiple mappings -->
  	<url-pattern>/api/*</url-pattern>
  </servlet-mapping>
  
  
</web-app>